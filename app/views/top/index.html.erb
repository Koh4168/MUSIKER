 <main role="main-search">
      <section class="section">
         <%= form_tag events_path, :method => 'get' do %>
          <%= text_field_tag :search1, '', class: 'main_search' %>
          <input  type="submit" value="&#xf002" class="main_search" /></td>
          <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
          <% end %>
  
          
      <%#元々の検索フォーム
      <form id="form1" action="https://e5f4290b9b1f4589af242e8d6c5dca04.vfs.cloud9.ap-northeast-1.amazonaws.com/?s=#">
        <input id="sbox"  id="s" name="s" type="text" placeholder="キーワードを入力" />
        <input id="sbtn" type="submit" value="検索" /></td>
        </form>%>
      </section>
    <%#イベントのピックアップ%>  
      <section class="pickup">
         <div class='pickup_font'>
             <h2 class='pp_font'>Pick up Event</h2>
             <div class='pickup_information'>開催間近、人気のコンサートです
             </div>
         </div>
          <div class="flexslider carousel">
            <ul class="slides">
               <% @events.each do |event| %>
              <li>
                 <%=link_to (image_tag event.image.url),"/events/#{event.id}"%>
              </li>
               <% end %>
            </ul>
          </div>
     
     <%# userのピックアップ%>     
        <div class='pickup_font'>
            <h2 class='pp_font'>Pick up Artist</h2>
                <div class='pickup_information'>今、人気のアーティスト
                </div>
            <div class="flexslider carousel">
                <ul class="slides">
                     <% @user.each do |user| %>
                          <li> 
                            <%=link_to (image_tag user.image.url),"/users/#{user.id}" %>
                          </li>
                         
                     <% end %>
                </ul>
          </div>
          
         </div>
      </section>

      <section class="comming">

      </section>
    </main>
    
 