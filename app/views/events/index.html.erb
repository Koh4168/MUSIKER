<div class="contents row">
    <% @events.each do |event| %>
     <p><%= event.name%></p>
     <p><%= event.text%></p>
     <p><%= event.image%></p>
     <p><%= event.place%></p>
     <p><%= event.price%></p>
     <p><%= event.scedule%></p>
    <% end %>
    
    
    <%= paginate(@events) %>
     
</div>